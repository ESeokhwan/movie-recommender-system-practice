{
  "editor.formatOnSave": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4
  },
  "black-formatter.args": [
    "--preview",
    "--enable-unstable-feature",
    "string_processing",
    "--enable-unstable-feature",
    "multiline_string_handling"
  ],
  "cSpell.words": ["decomp", "ndarray", "rsme"]
}
